{
  "version": "3.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "@picocss/pico@2.0.6",
      "destination": "wwwroot/lib/picocss",
      "files": [
        "css/pico.min.css",
        "LICENSE.md"
      ]
    },
    {
      "library": "remixicon@4.6.0",
      "destination": "wwwroot/lib/remixicon",
      "fileMappings": [
        {
          "files": ["License"]
        },
        {
          "root": "icons/Business",
          "files": [
            "mail-open-line.svg",
            "send-plane-2-fill.svg"
          ]
        },
        {
          "root": "icons/Document",
          "files": [
            "file-damage-line.svg"
          ]
        },
        {
          "root": "icons/Health & Medical",
          "files": [
            "heart-line.svg"
          ]
        },
        {
          "root": "icons/Media",
          "files": [
            "repeat-line.svg",
            "notification-2-line.svg"
          ]
        },
        {
          "root": "icons/System",
          "files": [
            "more-line.svg",
            "search-line.svg",
            "settings-3-line.svg"
          ]
        },
        {
          "root": "icons/User & Faces",
          "files": [
            "account-box-line.svg"
          ]
        }
      ]
    }
  ]
}
{
  "version": "1.0",
  "defaultProvider": "jsdelivr",
  "libraries": [
    {
      "library": "@picocss/pico@2.0.6",
      "destination": "wwwroot/lib/picocss",
      "files": [
        "css/pico.min.css",
        "LICENSE.md"
      ]
    },
    {
      "library": "mermaid@11.4.1",
      "destination": "wwwroot/lib/mermaid",
      "files": [
        "dist/mermaid.min.js",
        "LICENSE"
      ]
    },
    {
      "library": "prismjs@1.30.0",
      "destination": "wwwroot/lib/prismjs",
      "files": [
        "components/prism-core.min.js",
        "components/prism-csharp.min.js",
        "components/prism-markup.min.js",
        "components/prism-scss.min.js",
        "components/prism-javascript.min.js",
        "components/prism-bash.min.js",
        "components/prism-sql.min.js",
        "components/prism-yaml.min.js",
        "LICENSE"
      ]
    }
  ]
}